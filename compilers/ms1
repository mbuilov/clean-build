ifndef VCCL
ifndef MSVC
ifndef VS

  # known versions of Visual Studio
  VS_VERSIONS := 7.0 7.1 8.0 9.0 10.0 11.0 12.0 14.0 15.0

  # query registry for Visual Studio installation paths
  VS_REG_QUERY = $(lastword \
    $(subst HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\VisualStudio\SxS\VC7, ,$(subst \
    $(space),?,$(strip $(shell reg query "HKLM\SOFTWARE\Microsoft\VisualStudio\SxS\VC7")))))$(if $(IS_WIN_64), $(lastword \
    $(subst HKEY_LOCAL_MACHINE\SOFTWARE\Wow6432Node\Microsoft\VisualStudio\SxS\VC7, ,$(subst \
    $(space),?,$(strip $(shell reg query "HKLM\SOFTWARE\Wow6432Node\Microsoft\VisualStudio\SxS\VC7"))))))

  # $1 - 7.0 7.1 8.0 9.0 10.0 11.0 12.0 14.0 15.0
  # $2 - ?9.0?REG_SZ?C:\Program?Files?(x86)\Microsoft?Visual?Studio?9.0\VC\?FrameworkDir32?REG_SZ?C:\Windows\Microsoft.NET\Framework\
  #
  VS_SUBST_PATHS = $(if $1,$(call VS_SUBST_PATHS,$(wordlist 2,999999,$1),$(subst ?$(firstword $1)?REG_SZ?, $(firstword $1)?,$2)),$2)

  # get list of Visual Studio installation paths
  VS_REG_PLACES = $(sort $(filter-out \
    FrameworkVer% FrameworkDir%,$(subst \
    ?FrameworkVer, FrameworkVer,$(subst \
    ?FrameworkDir, FrameworkDir,$(call \
    VS_SUBST_PATHS,$(VS_VERSIONS),$(VS_REG_QUERY))))))

  # query registry for Visual Studio installation paths
  VS6_REG_QUERY = $(lastword \
    $(subst \SOFTWARE\Microsoft\VisualStudio\SxS\VC7, ,$(subst \
    $(space),?,$(strip $(shell reg query "HKLM\SOFTWARE\Microsoft\VisualStudio\6.0\Setup\Microsoft Visual C++" /v ProductDir)))))$(if $(IS_WIN_64), $(lastword \
    $(subst \SOFTWARE\Wow6432Node\Microsoft\VisualStudio\SxS\VC7, ,$(subst \
    $(space),?,$(strip $(shell reg query "HKLM\SOFTWARE\Wow6432Node\Microsoft\VisualStudio\6.0\Setup\Microsoft Visual C++" /v ProductDir)


  # check registry
  MSVC := $(shell reg query "HKLM\SOFTWARE\Microsoft\VisualStudio\6.0\Setup\Microsoft Visual C++" /v ProductDir)


